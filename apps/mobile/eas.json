{
  "cli": {
    "version": ">= 13.2.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "API_URL": "http://localhost:9000"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "API_URL": "https://wani-api-preview.railway.app"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_URL": "https://wani-api.railway.app"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "internal"
      },
      "ios": {
        "appleId": "dev@jappi.ca",
        "ascAppId": "placeholder",
        "appleTeamId": "placeholder"
      }
    }
  }
}
